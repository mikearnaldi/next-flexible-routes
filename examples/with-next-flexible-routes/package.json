    "monocle-ts": "^1.7.1",
    "next": "^8.0.3",
    "next-flexible-routes": "0.0.5",
    "path-to-regexp": "^3.0.0",
    "preact": "^8.4.2",
    "query-string": "^6.4.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "ts-node": "^8.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@types/express": "^4.16.1",
    "@types/next": "^8.0.3",
    "@types/react": "^16.8.10",
